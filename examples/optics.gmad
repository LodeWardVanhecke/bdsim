! some beamline parameters

option, beampipeRadius = 20 * cm,
        boxSize = 1.9 * m,
	tunnelRadius= 2.0 * m,
	beampipeThickness = 1 * cm;
	

include options.gmad;

! beamline definiton


qd: quadrupole,l=1 * m;
d: drift,l=1 * m;
s1: rcol, l=0.1*m, xaper=1*cm,yaper=1*cm;
endl:marker;

test_line : line = (qd,d,s1,d,d,endl);

beam, particle="e-", energy=10*MeV,
      distrType="gauss", sigmaX=0.1*m;

option, physicsList="em_standard";

use, period=test_line;
