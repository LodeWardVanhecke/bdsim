! some beamline parameters

option, beampipeRadius = 10 * cm;

! beamline definiton

kqd := -0.7;
kqf := 0.9;

sb: sbend, l=0.5*m, angle=0.5;
qd: quadrupole,l=2*m,k1=kqd;
qf: quadrupole,l=1*m,k1=kqf;
sex:sextupole,l=1*m,k2=0.4;
d: drift,l=1*m;
s1: spoiler;
a1: absorber;
endl: marker;

test_line : line = (sb,d,qf,d,qd,d,sex,d,a1,d,endl);

